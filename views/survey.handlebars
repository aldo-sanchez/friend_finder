<div class="container">
  <form action="#">
    <div class="row">
      <div class="input-field col s6">
        <input id="name" type="text" class="">
        <label for="name">name</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s6">
        <input id="image" type="text" class="">
        <label for="image">image url</label>
      </div>
    </div>
    {{#each questions}}
    <div class="row">
      <div class="col s12">
        <p>{{question}}</p>
      </div>
    </div>
    <div class="row">
      <div class="col s12">
        <p class="range-field">
          <input type="range" id="test5" min="0" max="10" />
        </p>
      </div>
    </div>
    {{/each}}
  </form>
</div>